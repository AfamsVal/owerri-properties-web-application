RewriteEngine on

#RewriteCond %{HTTPS} off
#RewriteRule ^(.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

DirectoryIndex indexx.php
RewriteRule ^home$ indexx.php [NC,L]
RewriteRule ^about$ aboutx.php [NC,L]
RewriteRule ^properties$ propertiesx.php [NC,L]
RewriteRule ^agents$ agentsx.php [NC,L]
RewriteRule ^marketplace$ marketplacex.php [NC,L]
RewriteRule ^contact-us$ contact-usx.php [NC,L]
RewriteRule ^login$ loginx.php [NC,L]
RewriteRule ^logout$ logoutx.php [NC,L]
RewriteRule ^forgot-password$ forgot-passwordx.php [NC,L]
RewriteRule ^category$ marketplacex.php [NC,L]
RewriteRule ^register$ registerx.php [NC,L]
RewriteRule ^my-account$ my_accountx.php [NC,L]
RewriteRule ^my-property$ my_propertyx.php [NC,L]
RewriteRule ^terms-conditions$ terms_conditionsx.php [NC,L]
RewriteRule ^admin-properties$ admin_propertiesx.php [NC,L]
RewriteRule ^faq$ faqx.php [NC,L]
RewriteRule ^admin-contact$ admin_contactx.php [NC,L]
RewriteRule ^visitors$ visitorsx.php [NC,L]
RewriteRule ^terms-conditions$ terms_conditionsx.php [NC,L]
RewriteRule ^single/([0-9a-zA-Z@_.]+)$ single.php?id=$1 [NC,L]

